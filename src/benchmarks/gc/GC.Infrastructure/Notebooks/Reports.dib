    public static Metric<TSource> ToMB<TSource>(Metric<TSource> m)
    {
        double conversion = m.Unit switch
        {
            "bytes" => 0.000_001,
            "MB" => 1,
            _ => throw new Exception("Not convertible to MB")
        };

        return new(extract: source => conversion * m.DoExtract(source),
            title: m.Title,
            unit: "MB"
            // Cap/offset handled by original metric
            );
    }


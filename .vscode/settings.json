{
    "files.exclude": {
        "**/__pycache__/**": true,
        "**/.vs/**": true,
        "**/bin/**": true,
        "**/obj/**": true,
        "**/packages/**": true,
        "**/.venv/**": true,
        "**/.dotnet/**": true,
    },
    "files.readonlyInclude": {
        "/eng/common/**": true,
    },
